{
  "name": "turnover",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "start-dev": "ts-node-dev --files src/app.ts",
    "frontend": "cd client && yarn dev",
    "prepare": "tsc",
    "build": "cd client && yarn && yarn build && cd .. && rm -rf ./public/* && mkdir -p ./public && cp -rf client/dist/* ./public && yarn && yarn prepare",
    "dev": "concurrently \"yarn start-dev\" \"yarn frontend\"",
    "knex:migrate:make": "knex --knexfile src/database/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
    "seed": "knex seed:run --specific=popData.ts --knexfile src/database/knexfile.ts"
  },
  "author": "Sanjeev Kumar",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.13",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@overnightjs/core": "^1.7.6",
    "@overnightjs/jwt": "^1.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-jwt": "^6.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "dotenv": "^16.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}
